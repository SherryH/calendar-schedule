{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": { "es6": true, "browser": true },
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "max-len": [2, 150, 2, { "ignoreUrls": true }],
      "comma-dangle": [1, "never"]
    }
}
    